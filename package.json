    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "nodemailer": "^7.0.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-intersection-observer": "^9.16.0",
